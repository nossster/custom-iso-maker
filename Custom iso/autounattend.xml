		<File path="%TEMP%\sounds.ps1">
$mountKey = 'DefaultUser';
New-PSDrive -PSProvider 'Registry' -Root 'HKEY_USERS' -Name 'HKU';
$excludes = Get-ChildItem -LiteralPath "HKU:\${mountKey}\AppEvents\EventLabels" |
    Where-Object -FilterScript { ($_ | Get-ItemProperty).ExcludeFromCPL -eq 1; } |
    Select-Object -ExpandProperty 'PSChildName';
Get-ChildItem -Path "HKU:\${mountKey}\AppEvents\Schemes\Apps\*\*" |
    Where-Object -Property 'PSChildName' -NotIn $excludes |
    Get-ChildItem -Include '.Current' | Set-ItemProperty -Name '(default)' -Value '';
Remove-PSDrive -Name 'HKU';
		</File>
		<File path="C:\Windows\Setup\Scripts\unattend-01.cmd">
ECHO ********** Installing Chrome
Rem install Chrome
powershell.exe -ExecutionPolicy ByPass -command  "&amp; {$P = $env:TEMP + '\chrome_installer.exe'; Invoke-WebRequest 'https://dl.google.com/chrome/install/latest/chrome_installer.exe' -OutFile $P; Start-Process -FilePath $P -Args '/silent /install' -Verb RunAs -Wait; Remove-Item $P}"
ECHO ********** Installing Framework 3.5
Rem install Framework 3.5
powershell.exe -ExecutionPolicy ByPass -command "Enable-WindowsOptionalFeature -Online -FeatureName "NetFx3" -All"
ECHO ********** Installing DirectPlay
Rem install DirectPlay
powershell.exe -ExecutionPolicy ByPass -command "Enable-WindowsOptionalFeature -Online -FeatureName "DirectPlay" -all"
Rem install DiretX
powershell.exe -ExecutionPolicy ByPass -command  "&amp; {$P = $env:TEMP + '\dxwebsetup.exe'; Invoke-WebRequest 'https://download.microsoft.com/download/1/7/1/1718CCC4-6315-4D8E-9543-8E28A4E18C4C/dxwebsetup.exe' -OutFile $P; Start-Process -FilePath $P -Args '/q' -Verb RunAs -Wait; Remove-Item $P}"

REM ECHO ********** Installing FACEIT AC
curl -o faceit.exe https://anticheat-client.faceit-cdn.net/FACEITInstaller_64.exe &amp;&amp; faceit.exe /silent /suppressmsgboxes /norestart /sp /v  &amp;&amp;  "C:\Program Files\FACEIT AC\faceitclient.exe" &amp;&amp; del faceit.exe
Reg DELETE "HKCU\Software\Microsoft\Windows\CurrentVersion\Run" /v "FACEIT" /f

ECHO ********** Отключаем новости в таскбаре
:: Disable Taskbar Feeds
Reg.exe add "HKCU\SOFTWARE\Microsoft\Windows\CurrentVersion\Feeds" /v "ShellFeedsTaskbarViewMode" /t REG_DWORD /d "2" /f

ECHO ********** Отключаем корзину
:: Do not move deleted files to the Recycle Bin
Reg add "HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\Explorer" /v "NoRecycleFiles" /t REG_DWORD /d "1" /f
:: Hide on taskbar MeetNow
Reg add "HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\Explorer" /v " HideSCAMeetNow" /t REG_DWORD /d "1" /f

Rem quire Windows Hello sign in for Microsoft Accounts: 0 - off, 2 - on
Reg add "HKLM\SOFTWARE\Microsoft\Windows NT\CurrentVersion\PasswordLess\Device" /v "DevicePasswordLessBuildVersion" /t REG_DWORD /d "0" /f


ECHO ********** Disable “Reserved Storage” on Windows 10
Reg.exe add "HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\ReserveManager" /v "ShippedWithReserves" /t REG_DWORD /d "0" /f

rem Disable Automatic Updates of Windows Store Apps
reg add HKLM\SOFTWARE\Policies\Microsoft\WindowsStore /v AutoDownload /t REG_DWORD /d 2 /f

REM ECHO ********** Complitly Disable WinKey Отключаю кнопку WIN Complitly Disable WinKey
Reg add "HKCU\Software\Microsoft\Windows\CurrentVersion\Policies\Explorer" /v "NoWinKeys" /t REG_DWORD /d "1" /f
Reg add "HKLM\SYSTEM\CurrentControlSet\Control\Keyboard Layout" /v "Scancode Map" /t REG_BINARY /d "00000000000000000300000000005be000005e0000000000" /f

ECHO ********** Power Management High Performance
REM Power Management High Performance
powercfg -import "%~dp0gamer_pc.pow" a81a33c9-6d83-493e-a38d-d5fa3c620915
powercfg /setactive a81a33c9-6d83-493e-a38d-d5fa3c620915

REM Do not turn off the screen never Do not to sleep
powercfg -Change -standby-timeout-ac 0
powercfg -change -monitor-timeout-ac 0


ECHO ********** Disable action power button
REM disable action power button
powercfg -setdcvalueindex SCHEME_CURRENT 4f971e89-eebd-4455-a8de-9e59040e7347 7648efa3-dd9c-4e3e-b566-50f929386280 0


ECHO ********** force Windows to use 100% of your network bandwidth
REM force Windows to use 100% of your network bandwidth
Reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\Psched" /v "NonBestEffortLimit" /t REG_DWORD /d "0" /f

REM if system on SSD drive - set 0, HDD - 3
REM Если система на SSD диске - установите 0, HDD - 3
SET Prefetch=0


REM Disable All Notifications from Windows Security
Reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows Defender Security Center\Notifications" /v "DisableNotifications" /t REG_DWORD /d "1" /f

REM Enable_Always_show_all_notification_area_icons_for_all_users
reg add "HKCU\Software\Microsoft\Windows\CurrentVersion\Policies\Explorer" /v "NoAutoTrayNotify" /t REG_DWORD /d "1" /f
reg add "HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\Explorer" /v "NoAutoTrayNotify" /t REG_DWORD /d "1" /f

rem disable_Microsoft_Edge_shortcut_creation_on_desktop
reg add "HKLM\Software\Microsoft\Windows\CurrentVersion\Explorer" /v "DisableEdgeDesktopShortcutCreation" /t REG_DWORD /d "1" /f

rem Disable_People_Bar_on_taskbar
reg add "HKCU\Software\Policies\Microsoft\Windows\Explorer" /v "HidePeopleBar" /t REG_DWORD /d "1" /f
reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\Explorer" /v "HidePeopleBar" /t REG_DWORD /d "1" /f

rem Disable_Pin_Store_to_Taskbar
reg add "HKCU\SOFTWARE\Policies\Microsoft\Windows\Explorer" /v "NoPinningStoreToTaskbar" /t REG_DWORD /d "1" /f
reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\Explorer" /v "NoPinningStoreToTaskbar" /t REG_DWORD /d "1" /f

rem  Remove both Search and Task View Icons from Windows 10 Taskbar
reg add "HKCU\Software\Microsoft\Windows\CurrentVersion\Search" /v "SearchboxTaskbarMode" /t REG_DWORD /d "0" /f
reg add "HKCU\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced" /v "ShowTaskViewButton" /t REG_DWORD /d "0" /f

REM Reset_Clear_Pinned_Apps_on_Taskbar

DEL /F /S /Q /A "%AppData%\Microsoft\Internet Explorer\Quick Launch\User Pinned\TaskBar\*"

REG DELETE HKCU\Software\Microsoft\Windows\CurrentVersion\Explorer\Taskband /F
REG add "HKCU\Software\Microsoft\Windows\CurrentVersion\Explorer\Taskband\AuxilliaryPins" /v "MailPin" /t REG_DWORD /d "0" /f
